<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bitcamp.java106.pms.dao.TeamDao">

	<resultMap type="Team" id="TeamMap">
		<id column="name" property="name" />
		<result column="dscrt" property="description" />
		<result column="max_qty" property="maxQty" />
		<result column="sdt" property="startDate" />
		<result column="edt" property="endDate" />
	</resultMap>
	
	<select id="selectList" resultMap="TeamMap" parameterType="map">
		select name, sdt, edt, max_qty
		from pms_team
	</select>

	<insert id="insert" parameterType="Team">
		insert into pms_team(name,dscrt,max_qty,sdt,edt)
		values(#{name},#{dscrt},#{max_Qty},#{startDate},#{endDate})
	</insert>
	<select id="selectOne" parameterType="int" resultMap="TeamMap">
		select
		dscrt, sdt, edt, max_qty
		from pms_team
		where name=#{value}
	</select>
	<update id="update" parameterType="Team">
		update pms_team set
		dscrt=#{description},
		max_qty=#{max_Qty},
		sdt=#{startDate},
		edt=#{endDate},
		where name=#{name}
	</update>
	<delete id="delete" parameterType="int">
		delete from pms_team
		where name=#{value}
	</delete>
</mapper>
